    .checkbtn{
      display: block;
    }
    .navul{
      position: fixed;
      width: 100%;
      height: 100vh;
      /* background: #CB0F80; */
      background: rgb(199, 34, 34);
      top: 80px;
      left: -100%;
      text-align: center;
      transition: all .5s;
      color:white;
    }
    .navsection .navul .navli{
