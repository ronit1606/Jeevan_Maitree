    font-size: 17px;
    padding: 7px 13px;
    border-radius: 3px;
    text-transform: uppercase;
    font-weight: bold;
}

.navsection .navul .navli .nava:hover{
    color:white;
    font-size: 1.8rem;
    transition: all 0.4s ease;
}

  
  .navli.active , .nava:hover{
    /* background: #CB0F80; */
    background: rgb(199, 34, 34);
    transition: .5s;
    color:white;
  }

  .navli.active{
    background-color: rgb(199, 34, 34);
    color: white;
  }

  
  .checkbtn{
    font-size: 30px;
    /* color: #CB0F80; */
