    margin-right: 20px;
    margin-top: 2rem;
}
.navsection .navul .navli{
    display: inline-block;
    line-height: 80px;
    /* margin: 0 5px; */
}

.navsection .navul .navli .nava{
    /* color: #CB0F80; */
