body {
    overflow-x: hidden;
}

.navsection {
    background: white;
    height: 120px;
    width: 100%;
    padding: 0;
    margin-top: 0;
}

img.logoimg {
    margin: auto;
    padding: 1rem;
    margin-left: 5rem;
    margin-top: 1.2rem;
}

.navsection .navul {
    float: right;
    margin-right: 20px;
    margin-top: 2rem;
}

.navsection .navul .navli {
    display: inline-block;
    line-height: 80px;
    /* margin: 0 5px; */
}

.navsection .navul .navli .nava {
    /* color: #CB0F80; */
    color: rgb(199, 34, 34);
    font-size: 17px;
    padding: 7px 13px;
    border-radius: 3px;
    text-transform: uppercase;
    font-weight: bold;
}

.navsection .navul .navli .nava:hover {
    color: white;
    font-size: 1rem;
    transition: all 0.4s ease;
}


.navli.active,
.nava:hover {
    /* background: #CB0F80; */
    background: rgb(199, 34, 34);
    transition: .5s;
    color: white;
}

.navli.active {
    background-color: rgb(199, 34, 34);
    color: white;
}


.checkbtn {
    font-size: 30px;
    /* color: #CB0F80; */
    color: rgb(199, 34, 34);
    float: right;
    line-height: 80px;
    margin-right: 40px;
    cursor: pointer;
    display: none;
}

#check {
    display: none;
}

@media (max-width: 952px) {
    .logo .logoimg {
        font-size: 30px;
        padding-left: 50px;
    }

    .navsection .navul .navli .nava {
        font-size: 16px;
    }
}

@media (max-width: 858px) {
    .checkbtn {
        display: block;
    }

    .navul {
        position: fixed;
        width: 100%;
        height: 100vh;
        /* background: #CB0F80; */
        background: rgb(199, 34, 34);
        top: 80px;
        left: -100%;
        text-align: center;
        transition: all .5s;
        color: white;
    }

    .navsection .navul .navli {
        display: block;
        margin: 50px 0;
        line-height: 30px;
    }

    .navsection .navul .navli .nava {
        font-size: 20px;
        color: white;
    }

    .nava.active {
        /* background:white;
      color: #CB0F80;
      transition: .5s; */
        text-decoration: underline;
        color: white;
    }

    .nava:hover {
        color: white;
    }

    #check:checked~.navul {
        left: 0;
    }
}

